var element_lat = document.getElementById('latitude');
var element_lng = document.getElementById('longitude');
var element_place = document.getElementById('place');
if ( (element_lat != null) && (element_lng != null) && (element_place != null)) {
var lat_value = parseFloat(element_lat.innerText);
var lng_value = parseFloat(element_lng.innerText);
var place = element_place.innerText;

if( (lat_value >= -90) && (lat_value <= 90) && (lng_value >= -180) && (lng_value <= 180) && (place.length > 0 ) ) {
var style  = {
width: "500px",
height: "400px",
lineHeight: "400px"
}
for (var i in style) {
document.getElementById("map").style[i] = style[i];
}

function baiduMap_initialize() {
var map = new BMap.Map('map');
var point = new BMap.Point(lng_value, lat_value);
map.centerAndZoom(point, 14);
var marker = new BMap.Marker(point);
map.addOverlay(marker);
var label = new BMap.Label(place, {offset: new BMap.Size(20, -10)});
marker.setLabel(label);

// add zoom control in the upper left corner
var top_left_navigation = new BMap.NavigationControl();
map.addControl(top_left_navigation);
}
}
}

