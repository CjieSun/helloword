<a class="hl" name="580" href="#580">580</a><b>static</b> <b>int</b> <a class="xf" name="numcmp"/><a href="/9.0.0_r3/s?refs=numcmp&amp;project=system" class="xf">numcmp</a>(<a href="/9.0.0_r3/s?defs=uint64_t&amp;project=system">uint64_t</a> a, <a href="/9.0.0_r3/s?defs=uint64_t&amp;project=system">uint64_t</a> b) {
<a class="l" name="581" href="#581">581</a>    <b>if</b> (a &lt; b) <b>return</b> -<span class="n">1</span>;
<a class="l" name="582" href="#582">582</a>    <b>if</b> (a &gt; b) <b>return</b> <span class="n">1</span>;
<a class="l" name="583" href="#583">583</a>    <b>return</b> 0;
<a class="l" name="584" href="#584">584</a>}
